{
  "name": "cozylife-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:frontend": "jest --config=jest.config.js",
    "test:backend": "jest --config=jest.server.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "cd server && npm run dev",
    "server:install": "cd server && npm install",
    "dev": "concurrently \"npm start\" \"npm run server\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "build:all": "npm run build && cd server && npm run build"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ngrx/signals": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.0",
    "@testing-library/angular": "^18.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "jasmine-core": "~5.9.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  }
}